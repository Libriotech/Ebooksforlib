		<li><a tabindex="-1" href="#">Separated link</a></li>
	</ul>
    </div>
	</div>

	<div class="span4">
	<div class="dropdown">
	<a class="btn dropdown-toggle" data-toggle="dropdown" href="#" style="width:93%;text-align:left;"><span class="caret pull-right"></span>Topputlistinger</a>
	<ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
		<li><a tabindex="-1" href="#">Action</a></li>
		<li><a tabindex="-1" href="#">Another action</a></li>
		<li><a tabindex="-1" href="#">Something else here</a></li>
		<li class="divider"></li>
		<li><a tabindex="-1" href="#">Separated link</a></li>
	</ul>
	</div>
	</div>

	<div class="span4">
	    <form class="navbar-search pull-left" style="width:100%;">
	<input type="text" class="search-query" placeholder="Søk" style="width:90%">
	</form>
	</div>
</div>
</div> <!-- end container -->

</div> <!-- end header-wrapper -->

<!-- h1>[% IF template.pagetitle %][% template.pagetitle %][% ELSE %][% vars.appname %][% END %]</h1 -->

