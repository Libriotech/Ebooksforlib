<!DOCTYPE html>
<html lang="no"><head>
  <meta charset="UTF-8">
  <title>EBIB</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
<link rel="stylesheet" href="/css/jquery-ui-1.10.3.custom.min.css" />
  <script src="/js/jquery-1.9.1.min.js"></script>   
<script src="/stars/jquery.MetaData.js"></script>
<script src="/stars/jquery.rating.pack.js"></script>
<link href="/stars/jquery.rating.css" rel="stylesheet">
  <script src="/js/jquery.dotdotdot-1.5.7-packed.js"></script>
<script src="/js/jquery-ui-1.10.3.custom.min.js"></script>
<link href="http://reader.digitalt.org/img/favicon.ico" rel="icon" type="image/x-icon" />  
     <script src="http://reader.digitalt.org/js/jquery.bxslider.js"></script>
   <link href="http://reader.digitalt.org/css/jquery.bxslider.css" rel="stylesheet" />
   
<!--  <script src="/js/jquery.bxslider.min.js"></script>
  <link href="/css/jquery.bxslider.css" rel="stylesheet" /> -->
  
  <script src="/js/bootstrap.min.js"></script>
  <script src="/js/lawnchair.js"></script>
  <link href="/css/bootstrap.min.css" rel="stylesheet" media="screen">
  <link href="/css/bootstrap-responsive.min.css" rel="stylesheet">
  <link rel="stylesheet" href="/css/font-awesome.min.css">
  <link href="http://reader.digitalt.org/css/style.css" rel="stylesheet" />
  <script type="text/javascript" src="//use.typekit.net/lrk7anq.js"></script>
  <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
  <script>

   jQuery(document).ready(function () {
     jQuery('.bxslider').bxSlider({
       minSlides: 8,
       maxSlides: 8,
       slideWidth: 146,
       slideMargin: 0
     });
     jQuery('.bxspinner').addClass('bxhidden');
     jQuery('.bxslider').removeClass('bxhidden');
     jQuery('.ellipsis').dotdotdot();
     
     $('#frontpage_sort').sortable({
		update: function(event, ui) {
			var frontpage_order = $(this).sortable('toArray').toString();
			$.get('/lists/order', { frontpage_order: frontpage_order });
		}
	});
    $( "#frontpage_sort" ).disableSelection();

     
   });

window.addEventListener('resize', resizeDelayed, false);
window.addEventListener('orientationChanged', resizeEvent, false);
var resizeclean=1;
function resizeDelayed(e) {
 if (resizeclean) {
   resizeclean=0;
   setTimeout(function() { resizeEvent(e)}, 2000);
 }
}
function resizeEvent(e) {
 jQuery('.ellipsis').dotdotdot()
 resizeclean=1;
}

  </script>
<style type="text/css">

/* a {color:#c12121;}
a:hover {color:#900;} */
#frontpage_sort { list-style-type: none; margin: 0; padding: 0; width: 60%; }
#frontpage_sort li { margin: 0 3px 3px 3px; padding: 0.4em; padding-left: 1.5em; font-size: 1.4em; height: 18px; }
#frontpage_sort li span { position: absolute; margin-left: -1.3em; }
</style>

<!-- <link rel="stylesheet" href="[% request.uri_base %]/css/style.css" /> -->
[% library.detail_head %]

<script type="text/javascript" src="http://w.sharethis.com/button/buttons.js"></script>
<script type="text/javascript">stLight.options({publisher: "ur-beb289ee-59c0-ab4c-3b99-ec5cf86755a0", doNotHash: false, doNotCopy: false, hashAddressBar: false});</script>
</head>
<body id="portal">
<div class="header-wrapper">
<div class="container-fluid header-container">

[% IF session.logged_in_user %]

    <!-- BRANDING, SØK OG TOPPNAVIGERING
    ************************************* -->
    
    <div class="container-fluid header-container">
    <div class="row-fluid">
    <div class="span12">
    <header>
	    <nav>
		    <div class="navbar navbar-inverse">
			    <div class="navbar-inner">
				    <a class="brand" href="/"><i class="icon-home"></i> <span class="library-name">[% session.chosen_library_name %]</span></a>
    					<!-- .btn-navbar is used as the toggle for collapsed navbar content -->
						<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</a>
						<div class="nav-collapse">
				    		<ul class="nav pull-right">
				    			<li>
				    				<form class="navbar-search" action="/search" method="GET">
										<input type="text" class="search-query" name="q" placeholder="[% l('Title / Author / ISBN') %]">
    								</form>
    							</li>
				    			<li>
				    				<a href="http://reader.digitalt.org/" class="mine-boker">
				    				<i class='icon-book'></i></a>
				   			 	</li>
						    	<li><a class="dropdown-toggle min-side" role="button" data-toggle="dropdown" href="#"><i class="icon-user"></i></a>
						    		<ul class="dropdown-menu" role="menu">
						    			<li><a href="/my" src="" data="Log out">Min side</a></li>
						    			<li><a href="/out" src="" data="Log out">Logg ut</a></li>
						    		</ul>
				 			   	</li>
						 		<li><a href="/page/help" src="" data="Hjelp" class="hjelp">
				 					<i class="icon-question-sign"></i></a>
						 		</li>
				  		  </ul>
				    </div>
			    </div>
		    </div>
	    </nav>

    </header>
    </div>
    </div>
    </div> <!-- end container -->


[% ELSE %]

<!-- BRANDING, SØK OG TOPPNAVIGERING
************************************* -->

    <div class="container-fluid header-container">
    <div class="row-fluid">
    <div class="span12">
    <header>
	    <nav>
		    <div class="navbar navbar-inverse">
			    <div class="navbar-inner">
				    <a class="brand" href="/">
				    	<i class="icon-home"></i> <span class="hidden-phone library-name">[% session.chosen_library_name %]</span>
				    </a>
				    <form class="navbar-search pull-left" style="margin-top:5px;" action="/search" method="GET">
						<input type="text" class="search-query" name="q" placeholder="[% l('Title / Author / ISBN') %]">
    				</form>
				    [%# Users need to have a chosen library before they can be offered to log in. %]
				    [% IF session.chosen_library %]
				    <div class="logged-out-info pull-right hidden-phone">[% l('Du er ikke logget inn.') %] <a href="/in">Logg inn</a> eller finn ut mer <a href="/page/about">om eBib</a></div>
				    [% END %]
			    </div>
		    </div>
	    </nav>

    </header>
    </div>
    </div>
    </div> <!-- end container -->


 
[% END %]

</div> <!-- end container-fluid header-container -->
</div> <!-- end header-wrapper -->

[% IF flash.error %]
    <div class="flash error">[% flash.error %]</div>
[% END %]
[% IF flash.info %]
    <div class="flash info">[% flash.info %]</div>
[% END %]

[% content %]

<!-- FOOTER
************************************* -->

<section id="footer">
<div class="footer-wrapper">
<div class="container">
	<div class="row">
		<div class="span3">
			<h1>eBib</h1>
		</div>
[% IF session.logged_in_user %]
		<div class="span3">
			<h5>(Footer meny)</h5>
				<ul class="footer-menu-list">
				<li><a href="#">Om eBib</a></li>
				<li><a href="#">Dette trenger du for å låne</a></li>
				<li><a href="/page/help" src="" data="Hjelp">Hjelp</a></li>
				<li><a href="#">Kontakt</a></li>
			</ul>
		</div>
		<div class="span3">
			<h5>(Innlogget meny)</h5>
			<ul class="footer-menu-list">
				<li><a href="http://reader.digitalt.org/">Mine bøker</a></li>
				<li><a href="/my">Min side</a></li>
				<li><a href="/out" src="" data="Log out">Logg ut</a></li>
			</ul>
		</div>
[% ELSE %]	
		<div class="span3 offset3">
			<h5>(Footer meny)</h5>
			<ul class="footer-menu-list">
				<li><a href="#">Om eBib</a></li>
				<li><a href="#">Dette trenger du for å låne</a></li>
				<li><a href="/page/help" src="" data="Hjelp">Hjelp</a></li>
				<li><a href="#">Kontakt</a></li>
			</ul>
		</div>
[% END %]
		<div class="span3">
		<h5>(Sosialt og språk)</h5>
		<ul class="footer-menu-list">
				<li><a href="#">fb</a></li>
				<li><a href="#"><!-- LANGUAGE CHOOSER -->
[% FOREACH key IN installed_langs.keys %]
    [% IF key == language_tag %]
        [% installed_langs.$key %]
    [% ELSE %]
        <a href="/lang?lang=[% key %]">[% installed_langs.$key %]</a>
    [% END %]
[% END %]</a></li>
			</ul>
		</div>
	</div>
	
	<div class="row">
		<div class="span12 center">
					
		</div>
	</div>
</div>
</div>
</section>

</body>
</html>
