<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>[% IF template.pagetitle %][% template.pagetitle %] > [% END %][% vars.appname %]</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<script src="/javascripts/jquery-1.9.1.min.js"></script>
<script src="/stars/jquery.MetaData.js"></script>
<script src="/stars/jquery.rating.pack.js"></script>
<link href="/stars/jquery.rating.css" rel="stylesheet">
<script src="/js/jquery.dotdotdot-1.5.7-packed.js"></script>
<link href="/css/bootstrap.min.css" rel="stylesheet" media="screen">
<link href="/css/bootstrap-responsive.min.css" rel="stylesheet">
<link href="http://reader.digitalt.org/css/style.css" rel="stylesheet">
<link rel="stylesheet" href="/css/font-awesome.min.css">
<script src="/js/bootstrap.min.js"></script>
<script src="/js/lawnchair.js"></script>
<script>
jQuery(document).ready(function () {
 jQuery('.ellipsis').dotdotdot();
});

window.addEventListener('resize', resizeDelayed, false);
window.addEventListener('orientationChanged', resizeEvent, false);
var resizeclean=1;
function resizeDelayed(e) {
if (resizeclean) {
resizeclean=0;
setTimeout(function() { resizeEvent(e)}, 2000);
}
}
function resizeEvent(e) {
jQuery('.ellipsis').dotdotdot()
resizeclean=1;
}
</script>
<link rel="stylesheet" href="[% request.uri_base %]/css/style.css" />
[% library.detail_head %]
</head>

<body id="portal">
<div class="header-wrapper">

<!-- BRANDING OG TOPPNAVIGERING
************************************* -->

<div class="container-fluid header-container">
<div class="row-fluid">
<div class="span12">
<header>

	<nav>
		<div class="navbar navbar-inverse">
			<div class="navbar-inner">
				<a class="brand hidden-phone" href="/"><i class="icon-building"></i> [% session.chosen_library_name %]</a>
				<ul class="nav pull-right">
				<li><a href="/" src="" data="Hjem"><i class="icon-home"></i></a></li>
				<li class="active"><a href="" src="" data="Søk"><i class="icon-search"></i></a></li>
				<li>
				    [% IF session.logged_in_user %]
                        <a href="/log/out" src="" data="Log out"><i class="icon-signout"></i></a>
                    [% ELSE %]
                        <a href="/log/in" src="" data="Log in"><i class="icon-signin"></i></a>
                    [% END %]
				</li>
				<li><a href="" src="" data="Kontakt"><i class="icon-envelope-alt"></i></a></li>
				<li><a href="" src="" data="Hjelp"><i class="icon-question-sign"></i></a></li>
				</ul>
			</div>
		</div>
	</nav>

</header>
</div>
</div>
</div> <!-- end container -->

<!-- SJANGER, utlistingER OG SØK
************************************* -->

<div class="container hidden-phone">
<div class="row add-top">
	<div class="span4">
	<div class="dropdown">
	<a class="btn dropdown-toggle" data-toggle="dropdown" href="#" style="width:93%;text-align:left;"><span class="caret pull-right"></span>Sjanger</a>
