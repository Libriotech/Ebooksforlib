    	<button type="submit" class="btn btn-success btn-login pull-right half-top" style="width:50%;">Logg inn</button>
    	<a class="glemt-pinkode half-top" href="#">Hjelp til<br />innloggingen?</a>

        </form>
        </div>
    </div>
    </div> <!-- end container -->
    </div> <!-- end logginn-wrapper -->

<!-- NY BRUKER? BANNER
************************************* -->

	[% IF !session.logged_in_user %]

	<div class="hanging-alert2" style="position:absolute;left:50%;margin-bottom:-33px;margin-left:-178px;">
		<a href="#"><strong>Ny Bruker?</strong> Dette trenger du for å låne <i class="icon-angle-right"></i></a>
	</div>

	[% END %]

 
[% END %]

</div> <!-- end container-fluid header-container -->
</div> <!-- end header-wrapper -->

[% IF flash.error %]
    <div class="flash error">[% flash.error %]</div>
[% END %]
[% IF flash.info %]
