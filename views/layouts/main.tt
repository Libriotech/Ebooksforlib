<!-- Ensure library defaults to lillevik; and enable the two-part login-field by setting 'chosen realm'. IOK 2013-05-31 -->

        [% SET chosen_realm='' %]
	[% IF session.chosen_library == 2 %]
	[% SET chosen_realm='lillevik' %]
	[% ELSIF session.chosen_library == 1 %]
	[% SET chosen_realm='storevik' %]
	[% END %]

[% IF chosen_realm == '' %]	
 <script>
 window.location = 'http://portal.digitalt.org/library/set/2?return_url=/';
 </script>
[% END %]


<body id="portal">
<div class="header-wrapper">
<div class="container-fluid header-container">

[% IF session.logged_in_user %]

    <!-- BRANDING OG TOPPNAVIGERING
    ************************************* -->

    <div class="container-fluid header-container">
    <div class="row-fluid">
    <div class="span12">
    <header>
<!-- SJANGER, LISTER OG SØK
************************************* -->

	    <nav>
		    <div class="navbar navbar-inverse">
			    <div class="navbar-inner">
				    <a class="brand" href="/"><i class="icon-home"></i> <span class="hidden-phone">[% session.chosen_library_name %]</span></a>
				        <form class="navbar-search pull-left" style="margin-top:5px;">
							<input type="text" class="search-query" placeholder="Søk">
    					</form>
				    <ul class="nav pull-right">
				    <li><a href="http://reader.digitalt.org/"><i class='icon-book'></i></a></li>
				    <li><a href="/log/out" src="" data="Log out"><i class="icon-signout"></i></a></li>
				 <!--   <li><a href="" src="" data="Kontakt"><i class="icon-envelope-alt"></i></a></li> -->
				 <!--   <li><a href="" src="" data="Hjelp"><i class="icon-question-sign"></i></a></li> -->
				    </ul>
			    </div>
		    </div>
	    </nav>

    </header>
    </div>
    </div>
    </div> <!-- end container -->

<!-- SJANGER, utlistingER OG SØK
*************************************
   
    [% IF vars.hide_dropdowns != 1 %]


    <div class="container hidden-phone">
    <div class="row add-top">
	    <div class="span4">
	    <div class="dropdown">
	    <a class="btn dropdown-toggle" data-toggle="dropdown" href="#" style="width:93%;text-align:left;"><span class="caret pull-right"></span>Genre</a>
	    <ul class="dropdown-menu">
	        [% FOREACH list IN vars.lists %]
	            [% IF list.is_genre == 1 %]
        		    <li><a href="/list/[% list.id %]">[% list.name %]</a></li>
    		    [% END %]
		    [% END %]
	    </ul>
        </div>
	    </div>

	    <div class="span4">
	    <div class="dropdown">
	    <a class="btn dropdown-toggle" data-toggle="dropdown" href="#" style="width:93%;text-align:left;"><span class="caret pull-right"></span>Lists</a>
	    <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
	        [% FOREACH list IN vars.lists %]
	            [% IF list.is_genre == 0 %]
        		    <li><a href="/list/[% list.id %]">[% list.name %]</a></li>
    		    [% END %]
		    [% END %]
	    </ul>
	    </div>
	    </div>

	    <div class="span4">
	        <form method="GET" action="/search" class="navbar-search pull-left" style="width:100%;">
	    <input type="text" name="q" id="q" value="[% params.q | html_entity %]" class="search-query" placeholder="Søk" style="width:90%">
	    </form>
	    </div>
    </div>
    </div> 
    [% END %]
-->

[% ELSE %]

    <!-- LOGG INN BANNER
    ************************************* -->

    <div class="logginn-wrapper">
    <div class="container content-container">
    <div class="row">
	    <div class="span8">
	    <img src="http://reader.digitalt.org/img/ebib-logo2.png" class="pull-left" style="height:64px;width:64px;margin-right:10px;" />
		    <h1>Lån ebøker gratis!<!-- span style="font-size:34px;font-weight:300;opacity: 0.3;"> BETA</span --></h1>
		    <!-- <img src="img/laan-gratis-txt.png" /> -->
		    <p class="lead-list hidden-phone">
		    <i class="icon-ok" style="font-size:15px;"></i> Hundrevis av ebøker til din telefon, nettbrett og pc<br />
		    <i class="icon-ok" style="font-size:15px;"></i> Ny norsk litteratur fra ditt lokale bibliotek<br />
		    <i class="icon-ok" style="font-size:15px;"></i> Ingen purregebyr eller tunge turer til biblioteket<br />
		    </p>
        </div>
	
	    <div class="span3">
	    <form method='post' action="/log/in">
		 <input type=hidden name=realm value='[%chosen_realm%]' />
        	<div class="login-container">
        		<div class="login-row1">
        			<input class="span3" type="text" id='username' name='username' placeholder="Lånenummer">
        		</div>
        		<div class="login-row2">
        			<input class="span3" type="password" id='password' name='password' placeholder="PIN-kode">
        		</div>
        	</div>
        	
