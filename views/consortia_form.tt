<fieldset>
<legend>Consortium details</legend>
<label for="name">Name:</label>
<input type="text" name="name" id="name" value="[% name %][% consortium.name %]" required/>
[% IF consortium.id %]
<input type="hidden" name="id" id="id" value="[% consortium.id %]"/>
[% END %]
<br />
<input type="submit" id="save" value="Save" />
</fieldset>
